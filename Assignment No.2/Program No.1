import java.util.Scanner;
class Assignment_No_2 
{
    public static void main(String []args)
    {
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        if(num % 2 == 0){
            System.out.println(num + " is an Even number.");
        }
        else{
            System.out.println(num + " is an Odd number.");
        }
    }
    
}
