import java.util.Scanner;
class Assignment_No_3{
    public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int sum = 0;
        while(num != 0){
            sum += num % 10;
            num /= 10;
        }
        System.out.println("The sum of digits of the number, you entered is: " + sum);
    }
}
