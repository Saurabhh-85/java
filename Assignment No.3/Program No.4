import java.util.Scanner;
class Assignment_No_3{
    public static void main(String []args){
        Scanner sc = new Scanner(System.in);
        int num = sc.nextInt();
        int is_Prime = 1;
        if(num == 0 || num == 1){
            is_Prime = 0;
        }
        else{
            for(int i = 2; i < num; i++){
                if(num % i == 0){
                    is_Prime = 0;
                }
            }
        }
        if(is_Prime == 1){
            System.out.println(num + " is a Prime number.");
        }
        else{
            System.out.println(num + " isn't a Prime number.");
        }
    }
}
