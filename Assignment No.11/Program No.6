import java.util.*;
public class StringsAssignment {
    public static void main(String[] args) {
        String[] names = {"Ravi", "Anita", "John", "Zara", "Meena", "David"};
        Arrays.sort(names);
        System.out.println("After sorting:");
        for (int i = 0; i < names.length; i++) {
            System.out.println(names[i]);
        }
    }
}
